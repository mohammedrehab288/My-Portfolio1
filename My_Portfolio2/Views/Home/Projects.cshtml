@{
    Layout = "_Layout";
}
@using My_Portfolio2.Models
@using My_Portfolio2.ViewModels
@model ProjectViewModel
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="~/bootstrap/css/style.css" />
    <link href="~/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <title>@ViewBag.Title</title>
</head>
<body>
    <section class="projects" id="projects">
        <h2 class="title">Projects</h2>
        <div class="content">
            <div class="project-card">
                <div class="project-image">
                <img src="~/image/Game.png" />
                </div>
                <div class="project-info">
                <p class="project-category">
                        @Model.project.description[0]<br>
                        @Model.project.description[1]<br>
                        @Model.project.description[2]
                </p>
                    <strong class="project-title">
                    <span>Project_GG Game(With my university team)</span>
                    <a href="https://github.com/AymanOHM/project_gg" class="more-details">More details</a>
                    </strong>
                </div>
            </div>
            <div class="project-card">
                <div class="project-image">
                <img src="~/image/Screenshot (228).png" />
                </div>
                <div class="project-info">
                <p class="project-category">
                    • Converting infix to prefix notation is crucial for systems where operator precedence or parentheses can complicate evaluation.<br>
                    • It leads to simpler and more efficient algorithms, especially in fields like compiler construction, symbolic mathematics, and computer architecture.
                </p>
                    <strong class="project-title">
                    <span>Infix to Prefix(With my university team)</span>
                        <a href="#" class="more-details">More details</a>
                    </strong>
                </div>
            </div>
            <div class="project-card">
                <div class="project-image">
                    <img src="~/image/Screenshot (229).png" />
                </div>
                <div class="project-info">
                    <p class="project-category">
                    • My first MVC project.<br>
                    • It was built using the ASP.NET Core MVC framework.
                    </p>
                    <strong class="project-title">
                        <span>My Portfolio website</span>
                        <a href="#" class="more-details">More details</a>
                    </strong>
                </div>
            </div>
        </div>
    </section>
</body>
</html>
